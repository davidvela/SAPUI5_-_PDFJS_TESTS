<mvc:View
	controllerName="nabi.demo.MyLibraryDemoApp.controller.Home"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m"
	height="100%">
	<Page 
		title="Using the nabi.m library"
		class="sapUiResponsiveContentPadding">
		<content>
			<VBox>
				
				<Button
					text="Go to nabi.m.ImageFileUploader"
					press="onGoToImageFileUploader"/>
					
				<Button
					text="Go to nabi.m.PDFViewer"
					press="onGoToPDFViewer"/>
				
				<Button
					text="Go to the old PDFViewer"
					press="onGoToOLPDFViewer"/>
			</VBox>
		</content>
	</Page>
</mvc:View>